"original":{"line":94,"column":4},"generated":{"line":204,"column":20}},{"source":"styles/main.scss","name":null,"original":{"line":95,"column":4},"generated":{"line":205,"column":4}},{"source":"styles/main.scss","name":null,"original":{"line":95,"column":4},"generated":{"line":205,"column":19}}],"sources":{"styles/project-preview.scss":"article.project-card {\n  display: flex;\n  flex-flow: row nowrap;\n  justify-content: space-between;\n  max-width: 90%;\n  margin: auto;\n  margin-top: 2em;\n  &:first-of-type {\n    margin-top: 0;\n  }\n\n  padding: 0.5em;\n  background-color: $color-bg-secondary;\n  border-style: solid;\n  border-width: 0px;\n  border-radius: 8px;\n  box-shadow: $box-shadow;\n\n  @media screen and (max-width: 550px) {\n    flex-direction: column;\n  }\n}\n\ndiv.project-card-content {\n  display: flex;\n  flex-flow: column nowrap;\n  align-items: flex-start;\n  justify-content: space-between;\n\n  margin-right: 1em;\n\n  .project-card-text {\n    * {\n      margin: 0;\n    }\n\n    h3 {\n      color: $color-text-dark-primary;\n      font-weight: 700;\n    }\n\n    h4 {\n      color: $color-text-dark-secondary;\n      font-weight: 600;\n    }\n\n    p {\n      margin-top: 0.75em;\n      color: $color-text-dark-secondary;\n    }\n  }\n\n  .project-card-tags {\n    display: flex;\n    flex-flow: row wrap;\n    justify-content: flex-start;\n    align-content: space-between;\n    * {\n      margin: 0;\n      margin-right: 1em;\n      margin-top: 0.25em;\n    }\n    &:last-child {\n      margin-right: 0;\n    }\n  }\n\n  .project-card-links {\n    margin-top: 0.6em;\n    display: flex;\n    flex-direction: row;\n\n    justify-content: flex-start;\n    * {\n      margin: 0;\n      margin-right: 1em;\n    }\n    &:last-child {\n      margin-right: 0;\n    }\n  }\n\n  @media screen and (max-width: 550px) {\n    margin-right: 0em;\n    align-items: center;\n    .project-card-text {\n      justify-content: center;\n      text-align: center;\n    }\n    .project-card-tags {\n      justify-content: center;\n    }\n    .project-card-links {\n      justify-content: center;\n    }\n  }\n}\n\nimg.project-card-img {\n  flex-shrink: 0;\n  align-self: center;\n  margin: 0;\n  width: 240px;\n  height: 240px;\n  @media screen and (max-width: 650px) {\n    width: 200px;\n    height: 200px;\n  }\n\n  @media screen and (max-width: 550px) {\n    order: -1;\n    margin-bottom: 0.75em;\n    width: 240px;\n    height: 240px;\n  }\n}\n","styles/variables.scss":"$color-bg-primary: #6c63fa;\n$color-bg-secondary: #fff; // #ececed;\n$color-text-light-primary: #fff; // #ececed;\n$color-text-dark-primary: #47484f;\n$color-text-dark-secondary: #656670;\n\n$button-color-primary: #fa6863;\n\n$tag-color-primary: #fab463;\n\n$sm: 481px; // Mobile devices\n$md: 769px; // Tablets\n$lg: 1025px; // Laptops\n\n$font-size: 18px;\n$header-font: \"Montserrat\", sans-serif;\n$header-font-weight: 800;\n$header-line-height: 1.4;\n$body-font: \"Hind\", sans-serif;\n$body-font-weight: 500;\n$body-line-height: 1;\n\n$box-shadow: 0px 0px 18px 5px rgba(0, 0, 0, 0.3);\n","styles/main.scss":"// @import \"modern-normalize/modern-normalize.css\";\n@import \"variables\";\n@import \"project-preview.scss\";\n\n* {\n  box-sizing: border-box;\n}\n\n* + * {\n  margin-top: 1em;\n}\n\nhtml,\nbody {\n  margin: 0;\n  font-family: $body-font;\n  font-weight: $body-font-weight;\n  line-height: $body-line-height;\n  font-size: $font-size;\n  color: $color-text-light-primary;\n  background-color: $color-bg-primary;\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  color: $color-text-light-primary;\n  line-height: $header-line-height;\n  font-family: $header-font;\n  font-weight: $header-font-weight;\n}\n\n.main-content {\n  margin: 2rem auto;\n  width: 100vw;\n  max-width: 750px;\n}\n\n.btn {\n  padding: 12px 12px;\n  text-align: center;\n  text-decoration: none;\n  font-size: 16px;\n  font-family: \"Montserrat\";\n  font-weight: 700;\n  border: none;\n  border-radius: 5px;\n  background-color: $button-color-primary;\n  color: $color-text-light-primary;\n\n  &.btn-github {\n    background-color: #24292e;\n    padding: 12px 12px 12px 45px;\n    color: #fff;\n    background-image: url(../images/github-icon-32px.png);\n    background-size: 25px auto;\n    background-repeat: no-repeat;\n    background-position: 12px center;\n\n    @media screen and (max-width: 650px) {\n      padding: 10px 10px 10px 35px;\n      background-position: 9px center;\n      background-size: 20px auto;\n    }\n  }\n\n  @media screen and (max-width: 650px) {\n    padding: 10px 10px;\n    font-size: 14px;\n  }\n}\n\n.section-header {\n  margin-left: 1rem;\n  margin-right: 1rem;\n  border-bottom: 4px solid #fff;\n}\n\n.tag {\n  padding: 4px 8px;\n  text-align: center;\n  text-decoration: none;\n  font-size: 16px;\n  font-family: \"Hind\", sans-serif;\n  font-weight: 600;\n  border: solid;\n  border-width: 3px;\n  border-radius: 15px;\n  color: $tag-color-primary;\n  @media screen and (max-width: 650px) {\n    padding: 3px 6px;\n    font-size: 14px;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"124699a2ba25696bbad51dbb3861685b","cacheData":{"env":{}}}